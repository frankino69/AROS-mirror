SRCS = autopilot.c \
       drivers.c \
       ui_helper.c \
       uinew.c \
       save.c \
       play.c \
       keywords.c \
       param.c

OBJS = $(SRCS:.c=.o)

include ../dos.mak
CFLAGS=$(CFLAGS1) -I ../include

TLIB = ../lib/libui.a


all:	$(TLIB)

$(TLIB):$(OBJS)
	rm -f $@
	$(AR) rc $@ $^

clean:
	rm -f $(TLIB)
	rm -f *.[oas]
	rm -f *~
	rm -f core 

distclean:clean

