SRCS = png.c \
       catalog.c \
       thread.c \
       xstdio.c \
       timers.c 

OBJS = $(SRCS:.c=.o)

include ../dos.mak
CFLAGS=$(CFLAGS1) -I ../include

TLIB = ../lib/libutil.a


all:	$(TLIB)

$(TLIB):$(OBJS)
	rm -f $@
	$(AR) rc $@ $^

clean:
	rm -f $(TLIB)
	rm -f *.[oas]
	rm -f *~
	rm -f core 

distclean:clean

