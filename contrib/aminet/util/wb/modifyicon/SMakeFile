SCOPTS  = RESOPT PARAMETERS=REGISTERS NOSTACKCHECK STRINGMERGE UNSIGNEDCHARS\
NOCHECKABORT OPTIMIZE OPTIMIZERSIZE OPTINLOCAL NOICONS MEMSIZE=HUGE #DEBUG=LINE NOOPT

SLOPTS  = SMALLDATA SMALLCODE NOICONS #STRIPDEBUG
SDILIB  = LIB:SDI_ASM_STD_functions.lib
FARPAR  = DATA=FARONLY CODE=FAR
FARLIB  = LIB:scnb.lib
NEARLIB = LIB:sc.lib
# *** ModifyIcon *******************************

ModifyIcon: T:ModifyIcon.o
	slink $(SLOPTS) FROM $? LIB $(FARLIB) TO $@
	Protect $@ +p

T:ModifyIcon.o: ModifyIcon.c
	sc $(SCOPTS) DATA=FARONLY $? OBJNAME=$@

